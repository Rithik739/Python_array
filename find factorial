def factorial(n):
    if n==0 or n==1:
        return 1
    else:
        return n*factorial(n-1)
    
n=5
a=factorial(n)
print(a)
/////////////////////////////////////////////////////////////////////////////////////////////////////////////////

///////////////using Built-In Function///////////////////////////////////////////////////////////////////////

import math

def factorial(n):
    return math.factorial(n)
    
n=int(input('enter the number '))
a=factorial(n)
print(a)
